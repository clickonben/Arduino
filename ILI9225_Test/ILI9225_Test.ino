String duration;
unsigned long startTime;
// Setup
void setup() {
  tft.begin();
  Serial.begin(9600);
}

// Loop
void loop() {
  startTime = millis();
  tft.drawBitmap(0, 0, gImage_ic, 176, 176, COLOR_CYAN);
  tft.setBackgroundColor(COLOR_BLACK);
  tft.setFont(Terminal6x8);
  tft.drawText(0, 178, "Printing some random text.", COLOR_WHITE);
  tft.drawText(0, 188, "Another line of nonsense", COLOR_WHITE);
  duration = String(millis()-startTime);
  tft.drawText(0, 198, duration, COLOR_WHITE);
  
  

  while(true);

}
